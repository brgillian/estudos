algoritmo "semnome"
// Função :
// Autor :
// Data : 26/12/2020
// Seção de Declarações 
var
  N: Inteiro
  C: Inteiro
  S: Inteiro
inicio
// Seção de Comandos
   S <- 0
   Para C <- 1 ate 5 faca
      EscrevaL("Digite um valor: ")
      Leia(N)
      S <- S + N
   FimPara
   EscrevaL("A soma dos valores e ", S)
fimalgoritmo
